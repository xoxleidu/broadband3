<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjts.broadband.job.dao.SpeedMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.zjts.broadband.job.model.Speed">
		<id column="id" property="id" />
		<result column="ip_id" property="ipId" />
		<result column="max_upload_speed" property="maxUploadSpeed" />
		<result column="max_download_speed" property="maxDownloadSpeed" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ip_id AS ipId, max_upload_speed AS maxUploadSpeed, max_download_speed AS maxDownloadSpeed
    </sql>

</mapper>
