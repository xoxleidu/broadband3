<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjts.broadband.job.dao.IpSegmentMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.zjts.broadband.job.model.IpSegment">
		<id column="id" property="id" />
		<result column="ip_name" property="ipName" />
		<result column="start_ip" property="startIp" />
		<result column="end_ip" property="endIp" />
		<result column="status" property="status" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ip_name AS ipName, start_ip AS startIp, end_ip AS endIp, status
    </sql>

</mapper>
